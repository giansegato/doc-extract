{
    "manifest_version": 3,
    "name": "Image Extractor",
    "version": "1.0",
    "description": "Extracts images from pages with specific class and creates a PDF",
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ],
    "host_permissions": [
      "https://docsend.com/view/*",
      "https://*.s3.amazonaws.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://docsend.com/view/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }